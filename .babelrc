{
    "presets": [
        // https://github.com/babel/babel-preset-env
        ["env", {
            "targets": {
                "browsers": ["> 5%", "last 2 versions", "not ie <= 8"]
            },
            // "modules": "commonjs", // by default
            "loose": true
        }],
        //支持stage-0的全部语法
        "stage-0"
    ],
    "plugins": [
        // http://babeljs.io/docs/plugins/transform-runtime/
        ["transform-runtime", {
            "polyfill": true,
            "regenerator": true
        }]
    ],
    // Output comments in generated output.
    "comments": true
}
